                         xsi:schemaLocation="http://www.intel.com/soae/localService/tokenExchange-2010a/ ../tokenExchange.xsd" 
                         tokenToDataOnly="false">
  <tkx:description/>
  <tkx:domainInstance domainId="domain-id-1" domainInstanceDesc="auto-generated">
    <tkx:tokenScheme>
      <tkx:dataDomain type="creditCardNumber"/>
      <tkx:tokenRules>
        <tkx:preserveHeaderTrailer headerPreserve="5" trailerPreserve="6"/>
      </tkx:tokenRules>
      <tkx:tokenFormat>
        <tkx:ccnFormat/>
